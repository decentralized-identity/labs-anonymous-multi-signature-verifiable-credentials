{
  "name": "zkmpa",
  "version": "0.0.1",
  "private": true,
  "description": "zkMPA - Zero-Knowledge Multi-Party Approval for Verifiable Credentials",
  "scripts": {
    "dev:example": "pnpm --parallel --filter @zkmpa-demo/backend --filter @zkmpa-demo/frontend dev",
    "dev:backend": "pnpm --filter @zkmpa-demo/backend dev",
    "dev:frontend": "pnpm --filter @zkmpa-demo/frontend dev",
    "build": "pnpm --recursive build",
    "lint": "pnpm --recursive lint",
    "test": "pnpm --recursive test",
    "build:packages": "pnpm run build:identity && pnpm run build:group && pnpm run build:proof && pnpm run build:storage && pnpm run build:proposal && pnpm run build:credential && pnpm run build:core",
    "build:identity": "pnpm --filter @zkmpa/identity build",
    "build:group": "pnpm --filter @zkmpa/group build",
    "build:proof": "pnpm --filter @zkmpa/proof build",
    "build:storage": "pnpm --filter @zkmpa/storage build",
    "build:proposal": "pnpm --filter @zkmpa/proposal build",
    "build:credential": "pnpm --filter @zkmpa/credential build",
    "build:core": "pnpm --filter @zkmpa/core build"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/anon-multiparty-vc"
  },
  "keywords": [
    "verifiable-credentials",
    "semaphore",
    "zero-knowledge",
    "multi-party",
    "anonymous-voting",
    "did",
    "veramo"
  ],
  "author": "",
  "license": "MIT"
}
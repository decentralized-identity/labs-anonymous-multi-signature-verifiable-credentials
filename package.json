{
  "name": "zkmap",
  "version": "0.0.1",
  "private": true,
  "description": "zkMAP - Zero-Knowledge Multi-party Approval Protocol for Verifiable Credentials",
  "scripts": {
    "dev:example": "cd examples/zkmap-demo && pnpm dev:all",
    "build": "pnpm --recursive build",
    "lint": "pnpm --recursive lint",
    "test": "pnpm --recursive test",
    "build:packages": "pnpm run build:identity && pnpm run build:group && pnpm run build:proof && pnpm run build:storage && pnpm run build:proposal && pnpm run build:credential && pnpm run build:core",
    "build:identity": "pnpm --filter @zkmap/identity build",
    "build:group": "pnpm --filter @zkmap/group build",
    "build:proof": "pnpm --filter @zkmap/proof build",
    "build:storage": "pnpm --filter @zkmap/storage build",
    "build:proposal": "pnpm --filter @zkmap/proposal build",
    "build:credential": "pnpm --filter @zkmap/credential build",
    "build:core": "pnpm --filter @zkmap/core build"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/anon-multiparty-vc"
  },
  "keywords": [
    "verifiable-credentials",
    "semaphore",
    "zero-knowledge",
    "multi-party",
    "anonymous-voting",
    "did",
    "veramo"
  ],
  "author": "",
  "license": "MIT"
}